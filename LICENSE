#include <stdio.h>
#include <stdlib.h>
#include <omp.h>
#define TRUE 1

void lihatNotes();
void tambahNote();
void hapusNote();
void editProgress();
void cariNote();
void urutkanNote();
void imporNote();
void eksporNote();

int main(){
	int menu_pilihan;
	
	printf("1. Keluar dari program\n2. Melihat notes\n3. Menambahkan note\n4. Menghapus note\n5. Mengedit progress\n6. Mencari note\n6. Mengurutkan note\n7. Mengimpor note\n8. Mengekspor note\n\nInput:");
	scanf("%d", &menu_pilihan);
	
	while (TRUE){ 
		switch(menu_pilihan){
		case 0:
			break;
			return 0;
		case 1:
			lihatNotes();
			break;
		case 2:
			tambahNote();
			break;
		case 3:
			hapusNote();
			break;
		case 4:
			editProgress();
			break;
		case 5:
			cariNote();
			break;
		case 6:
			urutkanNote();
			break;
		case 7:
			imporNote();
			break;
		case 8:
			eksporNote();
			break;	
		}
	}
}
